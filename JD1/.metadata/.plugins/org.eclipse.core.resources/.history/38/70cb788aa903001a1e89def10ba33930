package sam.java.book;

public class DooBee {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		
		int x = 1;
		while (x<5) {
			System.out.println ("Doo");
			System.out.println ("Bee");
			x++;
		}
		if (x == 5) {
			
			System.out.print("Do");
			
		}
		
		
	}

}
